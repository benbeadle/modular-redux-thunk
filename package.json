{
  "name": "modular-redux-thunk",
  "version": "1.0.0-beta3",
  "description": "A ducks-inspired package to help organize actions, reducers, and selectors together.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm test && npm run clean:build && babel src --presets es2015 --out-dir lib",
    "clean:build": "rm -rf lib",
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register ./specs/src/**/*.spec.js",
    "test:build": "mocha ./specs/build/**/*.spec.js",
    "test:watch": "nodemon -x \"npm test\" -e js -w src -w specs",
    "!!test:cover": "istanbul cover _mocha -- ./lib/specs/**/*.spec.js",
    "!!cover": "BABEL_ENV=test istanbul cover _mocha -- -u exports --compilers js:babel-core/register specs",
    "coveralls": "NODE_ENV=test BABEL_ENV=test istanbul cover _mocha --report lcovonly -- -u exports --compilers js:babel-core/register --recursive specs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benbeadle/modular-redux-thunk.git"
  },
  "keywords": [
    "redux",
    "thunk",
    "ducks",
    "reducers",
    "actions",
    "selectors",
    "store"
  ],
  "author": "Ben Beadle <ben@benbeadle.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benbeadle/modular-redux-thunk/issues"
  },
  "homepage": "https://github.com/benbeadle/modular-redux-thunk",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.1",
    "mocha-lcov-reporter": "^1.2.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-freeze": "^0.1.4",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.5",
    "warning": "^3.0.0"
  }
}
